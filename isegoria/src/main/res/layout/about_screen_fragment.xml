<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff262e3b">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView10" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="128dp"
                android:layout_height="128dp"
                android:layout_gravity="center_horizontal"
                android:src="@drawable/welcome"
                android:paddingTop="20dp"
                 />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Isegoria"
                android:id="@+id/textView7"
                android:textAlignment="center"
                android:layout_gravity="center_horizontal"
                android:textColor="#ffffffff"
                android:paddingTop="10dp"
                android:textSize="35dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Version 1.0 &quot;Tesla&quot; Beta"
                android:id="@+id/textView8"
                android:textAlignment="center"
                android:layout_gravity="center_horizontal"
                android:textColor="#ffffffff"
                android:paddingTop="10dp"
                android:textSize="16dp" />

            <LinearLayout
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:paddingTop="7dp"
                android:paddingBottom="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Directed By"
                    android:id="@+id/textView9"
                    android:layout_gravity="center_vertical"
                    android:textSize="19dp"
                    android:textColor="#ff4c74cc"
                    android:paddingRight="8dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Miguel Wood"
                    android:id="@+id/textView10"
                    android:layout_gravity="center|right"
                    android:textSize="19dp"
                    android:textColor="#fffff0ef" />
            </LinearLayout>

            <LinearLayout
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:paddingBottom="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Android Client"
                    android:id="@+id/textView9"
                    android:layout_gravity="center_vertical"
                    android:textSize="19dp"
                    android:textColor="#ff4c74cc"
                    android:paddingRight="8dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Anthony Quattrone"
                    android:id="@+id/textView10"
                    android:layout_gravity="center|right"
                    android:textSize="19dp"
                    android:textColor="#fffff0ef" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:gravity="center"
                android:orientation="horizontal"
                android:id="@+id/textView2">

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="1dip"
                    android:background="#51729F"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="fill_parent">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Behavioral Team"
                        android:id="@+id/textView11"
                        android:textSize="19dp"
                        android:textColor="#ff4c74cc"
                        android:paddingLeft="20dp"
                        android:paddingTop="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="James Kotsias"
                        android:id="@+id/textView13"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:paddingLeft="20dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Drew Smith"
                        android:id="@+id/textView14"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:paddingLeft="20dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Greg Newitt"
                        android:id="@+id/textView15"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:paddingLeft="20dp"
                        android:layout_marginRight="18dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Miguel Wood"
                        android:id="@+id/textView16"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:paddingLeft="20dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Charlie Eastaugh"
                        android:id="@+id/textView17"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:paddingLeft="20dp"
                        android:layout_marginRight="18dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Design Team"
                        android:id="@+id/textView23"
                        android:textSize="19dp"
                        android:textColor="#ff4c74cc"
                        android:paddingLeft="20dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Drew Smith"
                        android:id="@+id/textView24"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:paddingLeft="20dp"
                        android:layout_marginRight="18dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView25"
                        android:textSize="22dp"
                        android:textColor="#fffff0ef"
                        android:paddingLeft="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Coding Team"
                        android:id="@+id/textView12"
                        android:textSize="19dp"
                        android:textColor="#ff4c74cc"
                        android:layout_gravity="right"
                        android:paddingRight="20dp"
                        android:paddingTop="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Alan Gao"
                        android:id="@+id/textView18"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:layout_gravity="right"
                        android:paddingRight="20dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="David Rivera"
                        android:id="@+id/textView19"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:layout_gravity="right"
                        android:paddingRight="20dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Greg Newitt"
                        android:id="@+id/textView20"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:layout_gravity="right"
                        android:paddingRight="20dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Anthony Quattrone"
                        android:id="@+id/textView21"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:layout_gravity="right"
                        android:paddingRight="20dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Yikai Gong"
                        android:id="@+id/textView22"
                        android:textSize="18dp"
                        android:textColor="#fffff0ef"
                        android:layout_gravity="right"
                        android:paddingRight="20dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:text="Special Thanks To"
                        android:id="@+id/textView26"
                        android:textSize="19dp"
                        android:textColor="#ff4c74cc"
                        android:layout_gravity="center_vertical|right"
                        android:layout_marginRight="20dp"
                        android:paddingBottom="5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="The Eulers Bridge\nAdvisory Network Members"
                        android:id="@+id/textView27"
                        android:textSize="15dp"
                        android:textColor="#fffff0ef"
                        android:singleLine="false"
                        android:paddingRight="20dp" />
                </LinearLayout>

            </LinearLayout>


        </LinearLayout>
    </ScrollView>

</LinearLayout>